{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  {
    "env": { "browser": true, "es2022": true },
    "extends": [
      "eslint:recommended",
      "plugin:import/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": ["import"],
    "parserOptions": { "ecmaVersion": 2022, "sourceType": "module" },
    "settings": {
      "import/resolver": {
        "node": { "extensions": [".js"] }
      }
    },
    "rules": {
      "no-unused-vars": ["warn", { "argsIgnorePattern": "^_" }],
      "no-console": "off",
      "import/no-unresolved": "off"
    }
  }
  "rules": {
    "no-unused-vars": ["warn"],
    "no-console": "off"
  }
}
